{
  "name": "drmvc/app",
  "type": "project",
  "description": "Skeleton of application based on DrMVC Framework",
  "homepage": "https://drmvc.com/",
  "license": "MIT",
  "keywords": [
    "drmvc",
    "example",
    "application",
    "dotenv",
    "twig",
    "gulp"
  ],
  "autoload": {
    "psr-4": {
      "MyApp\\": "./app/"
    }
  },
  "authors": [
    {
      "name": "Paul Rock",
      "email": "paul@drteam.rocks",
      "homepage": "https://drteam.rocks/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "drmvc/framework": "^3.0.0@RC",
    "vlucas/phpdotenv": "^2.4.0",
    "twig/twig": "^2.4"
  },
  "minimum-stability": "rc",
  "scripts": {
    "dotenv": [
      "cp .env.example .env"
    ],
    "post-install-cmd": [
      "@dotenv"
    ]
  }
}
